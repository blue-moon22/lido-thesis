	month = jan,
	year = {2017},
	keywords = {Biochemical networks, Computer modelling, Microbial communities},
	pages = {81--89},
	file = {Full Text PDF:/Users/victoriabutt/Library/Application Support/Zotero/Profiles/3jt4koij.default/zotero/storage/ICEUT5TA/Magnúsdóttir et al. - 2017 - Generation of genome-scale metabolic reconstructio.pdf:application/pdf;Snapshot:/Users/victoriabutt/Library/Application Support/Zotero/Profiles/3jt4koij.default/zotero/storage/9FHBBFJF/nbt.3703.html:text/html}

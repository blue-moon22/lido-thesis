	month = apr,
	year = {2012},
	pages = {455--477},
	file = {Full Text PDF:/Users/victoriabutt/Library/Application Support/Zotero/Profiles/3jt4koij.default/zotero/storage/W9VAKM2C/Bankevich et al. - 2012 - SPAdes A New Genome Assembly Algorithm and Its Ap.pdf:application/pdf;Snapshot:/Users/victoriabutt/Library/Application Support/Zotero/Profiles/3jt4koij.default/zotero/storage/WERS5T7X/cmb.2012.html:text/html}

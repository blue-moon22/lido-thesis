	issn = {0888-7543},
	shorttitle = {{AlienTrimmer}},
	url = {http://www.sciencedirect.com/science/article/pii/S0888754313001481},
	doi = {10.1016/j.ygeno.2013.07.011},
	abstract = {Contaminant oligonucleotide sequences such as primers and adapters can occur in both ends of high-throughput sequencing (HTS) reads. AlienTrimmer was developed in order to detect and remove such contaminants. Based on the decomposition of specified alien nucleotide sequences into k-mers, AlienTrimmer is able to determine whether such alien k-mers are occurring in one or in both read ends by using a simple polynomial algorithm. Therefore, AlienTrimmer can process typical HTS single- or paired-end files with millions of reads in several minutes with very low computer resources. Based on the analysis of both simulated and real-case Illumina®, 454™ and Ion Torrent™ read data, we show that AlienTrimmer performs with excellent accuracy and speed in comparison with other trimming tools. The program is freely available at ftp://ftp.pasteur.fr/pub/gensoft/projects/AlienTrimmer/.},
	number = {5–6},
	urldate = {2017-02-15},
	journal = {Genomics},
	author = {Criscuolo, Alexis and Brisse, Sylvain},
	month = nov,
	year = {2013},
	keywords = {Adapter oligonucleotides, High-throughput sequencing, k-mer decomposition, Polynomial algorithm, Raw read trimming, Short contaminant sequence},
	pages = {500--506},
	annote = {How different from Trimmomatic: searches for an alien k-mer by specifying k (probably not as fast as Trimmomatic)},
	file = {ScienceDirect Full Text PDF:/Users/victoriabutt/Library/Application Support/Zotero/Profiles/3jt4koij.default/zotero/storage/5UI54ZZP/Criscuolo and Brisse - 2013 - AlienTrimmer A tool to quickly and accurately tri.pdf:application/pdf;ScienceDirect Snapshot:/Users/victoriabutt/Library/Application Support/Zotero/Profiles/3jt4koij.default/zotero/storage/I3A66PBE/S0888754313001481.html:text/html}
